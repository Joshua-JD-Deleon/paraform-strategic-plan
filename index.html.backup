        .framework-card--1 .framework-list li::before {
            color: var(--phase-30-day);
        }

        .framework-card--2 .framework-list li::before {
            color: var(--phase-60-day);
        }

        .framework-card--3 .framework-list li::before {
            color: var(--brand-accent);
        }

        .framework-card--4 .framework-list li::before {
            color: var(--phase-90-day);
        }

        /* === NAVIGATION SYSTEM === */
        .nav {
            position: fixed;
            top: 50%;
            right: var(--space-xl);
            transform: translateY(-50%);
            z-index: 1000;
            display: flex;
            flex-direction: column;
            gap: var(--space-sm);
        }

        .nav__dot {
            width: 12px;
            height: 12px;
            border-radius: 50%;
            background: var(--gray-300);
            border: none;
            cursor: pointer;
            transition: transform var(--motion-fast), background-color var(--motion-fast);
            position: relative;
            padding: 0;
            margin: 0;
        }

        .nav__dot:hover,
